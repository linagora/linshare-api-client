{
  "name": "linshare-api-client",
  "version": "0.0.18",
  "description": "JS library for Linshare APIs",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint && npm run build",
    "lint": "eslint src webpack.*.js",
    "build-dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "dist": "rm dist -rf && npm run build && npm run build-dev"
  },
  "keywords": [
    "linshare",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linagora/linshare-api-client"
  },
  "author": "Linagora folks <dev@linagora.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "es6-promise": "4.2.4",
    "eslint": "3.10.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "uglifyjs-webpack-plugin": "1.1.6",
    "webpack": "3.10.0",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "axios": "0.17.1"
  }
}
